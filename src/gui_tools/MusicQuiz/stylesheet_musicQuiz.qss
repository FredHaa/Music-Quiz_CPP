/****************************************/
/*****			 QDialog			*****/
/****************************************/

QDialog#QuizBoard
{
	color				: Yellow;
	background-color	: rgb(0, 0, 100);
}

QDialog#QuizSelector
{
	color				: Yellow;
	background-color	: rgb(25, 25, 255);
}

QDialog#TeamSelector
{
	color				: Yellow;
	background-color	: rgb(25, 25, 255);
}

QDialog#QuizIntroScreen
{
	color				: Yellow;
	background-color	: rgb(25, 25, 255);
}

QDialog#QuizWinningScreen
{
	color				: Yellow;
	background-color	: rgb(25, 25, 255);
}



/****************************************/
/*****		    QPushButton			*****/
/****************************************/

QPushButton[objectName^="QuizEntry"]
{
	color				: Yellow;
	font-size			: 40px;

	background-color	: rgb(0, 0, 255);

	border-color		: rgb(0, 0, 255);
	border-radius		: 0px;
	border-width		: 1px;
	border-style		: solid;
	margin				: 0px;
	padding				: 0px;

	min-height			: 100px;
	min-width			: 200px;
}

QPushButton[objectName^="TeamEntry"]
{
	font-size			: 40px;
	font-weight			: bold;

	min-height			: 100px;
	min-width			: 200px;
}

QPushButton[objectName^="_dayliDouble"]
{
	border-color		: rgb(255, 215, 0);
}

QPushButton#QuizEntry_categoryLabel
{
	font-size			: 40px;
	font-weight			: bold;
}

QPushButton#QuizEntry_rowCategoryLabel
{
	font-size			: 40px;
	font-weight			: bold;

	min-width			: 200px;
	min-height			: 100px;
}

QPushButton#settingsInfo
{
	min-width			: 30px;
	max-width			: 30px;
	min-height			: 30px;
	max-height			: 30px;
}

QPushButton#menuButton
{
	color				: Yellow;
	background-color	: rgb(96, 96, 96);
	font-size			: 30px;

	min-height			: 75px;
	min-width			: 200px;
}

QPushButton#addButton
{
	color				: Yellow;
	background-color	: rgb(96, 96, 96);
	font-size			: 50px;

	min-height			: 100px;
	min-width			: 100px;
}

QPushButton#removeBtn
{
	background-color	: rgba(0, 0, 0, 0);

	min-height			: 50px;
	min-width			: 50px;

	image				: url(:/imgs/remove_icon.png);
}

QPushButton#quizCreatorRemoveBtn
{
	font-size			: 10px;

	min-height			: 15px;
	min-width			: 15px;
}

QPushButton#quizCreatorBrowseBtn
{
	font-size			: 10px;

	min-height			: 40px;
	min-width			: 40px;

	image				: url(:/imgs/browse_file_icon.png);
}

QPushButton#quizCreatorPlayBtn
{
	font-size			: 10px;

	min-height			: 40px;
	min-width			: 40px;

	image				: url(:/imgs/play_icon.png);
}

QPushButton#quizCreatorPauseBtn
{
	font-size			: 10px;

	min-height			: 40px;
	min-width			: 40px;

	image				: url(:/imgs/pause_icon.png);
}

QPushButton#quizCreatorStopBtn
{
	font-size			: 10px;

	min-height			: 40px;
	min-width			: 40px;

	image				: url(:/imgs/stop_icon.png);
}



/****************************************/
/*****		    QLabel  			*****/
/****************************************/

QLabel#descriptionLabel
{
	font-size			: 45px;
	font-weight			: bold;
	color				: Yellow;
}

QLabel#categoryLabel
{
	font-size			: 35px;
	font-weight			: bold;
	color				: Yellow;
}

QLabel#infoLabel
{
	font-size			: 35px;
	font-weight			: bold;
	color				: Yellow;
}

QLabel#teamInfoLabel
{
	font-size			: 45px;
	font-weight			: bold;
	color				: Yellow;
}

QLabel#settingsLabel
{
	font-size			: 18px;
	color				: Black;
}

QLabel#authorLabel
{
	font-size			: 25px;
	color				: Yellow;
}

QLabel#quizIntroLabel
{
	font-size			: 100px;
	color				: Yellow;
}

QLabel#winningScreenTextLabel
{
	font-size			: 100px;
	color				: Yellow;
}

QLabel#quizCreatorLabel
{
	font-size			: 25px;
}



/****************************************/
/*****		    QListWidget			*****/
/****************************************/

QListWidget
{
	font-size						: 22px;
	background-color				: rgb(5, 30, 190);
	selection-background-color		: rgb(150, 150, 150);	
	border-color					: rgb(128, 128, 128);
	border-radius					: 0px;
	border-width					: 2px;
	border-style					: solid;
}

QListWidget::Item
{
	color							: Yellow;
	border							: 1px solid rgb(128, 128, 128)
}

QListWidget::Item::Selected
{
	border							: 5px solid Yellow
}


/****************************************/
/*****		    QTextEdit			*****/
/****************************************/

QTextEdit#descriptionTextLabel
{
	color							: Yellow;
	font-size						: 30px;
	background-color				: rgba(0, 0, 0, 0);
	selection-background-color		: rgba(0, 0, 0, 0);
	border							: none;
}

QTextEdit#quizCreatorTextEdit
{
	font-size						: 20px;
	min-height						: 80px;
}



/****************************************/
/*****		    QLineEdit  			*****/
/****************************************/

QLineEdit:disabled
{
	background-color				: rgb(100, 100, 100);
}

QLineEdit#authorText
{
	font-size						: 25px;
	background-color				: rgba(0, 0, 0, 0);
	selection-background-color		: rgba(0, 0, 0, 0);
	border							: none;
}

QLineEdit#_teamLineEdit
{
	font-size						: 35px;
	min-height						: 100px;
	border							: 2px solid rgb(128, 128, 128)
}

QLineEdit#quizCreatorLineEdit
{
	font-size						: 20px;
	min-height						: 40px;
}



/****************************************/
/*****		    QCheckBox  			*****/
/****************************************/

QCheckBox#infoCheckbox
{
	font-size	: 30px;
	color		: Yellow;
}

QCheckBox::indicator#infoCheckbox {
	width		: 50px;
	height		: 50px;
}

QCheckBox::indicator:checked#infoCheckbox {
	image		: url(:/imgs/checkbox_checked_yellow.png);
}

QCheckBox::indicator:unchecked#infoCheckbox {
	image		: url(:/imgs/checkbox_unchecked_yellow.png);
}



/****************************************/
/*****		    QTableWidget		*****/
/****************************************/

QTableWidget#teamTable
{
	font-size				: 32px;
	font-weight				: bold;
	background-color		: rgb(5, 30, 190);
	border-color			: rgb(128, 128, 128);
	border-radius			: 0px;
	border-width			: 2px;
	border-style			: solid;
}



/****************************************/
/*****		    QMessageBox			*****/
/****************************************/

QMessageBox
{
	font-size			: 25px;
}



/****************************************/
/*****		    QSpinBox			*****/
/****************************************/

QSpinBox#quizCreatorSpinbox
{
	font-size						: 20px;
	min-height						: 40px;
}



/****************************************/
/*****		    QTimeEdit			*****/
/****************************************/

QTimeEdit#quizCreatorTimeEdit
{
	font-size						: 20px;
	min-height						: 40px;
}



/****************************************/
/*****		    QRadioButton		*****/
/****************************************/

QRadioButton#quizCreatorRadioButton
{
	font-size						: 20px;
	min-height						: 40px;
}


